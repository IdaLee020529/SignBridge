{
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@react-three/drei": "^9.105.4",
    "@react-three/fiber": "^8.16.2",
    "gsap": "^3.12.5",
    "lucide-react": "^0.376.0",
    "mdb-react-ui-kit": "^8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd client && npm run dev",
    "start:backend": "cd server && node app",
    "install:all": "concurrently \"npm run install:root\" \"npm run install:frontend\" \"npm run install:backend\"",
    "install:root": "npm i",
    "install:frontend": "cd client && npm i",
    "install:backend": "cd server && npm i"
  }
}
